<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.securenotesapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Secure Notes Application"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Secure Notes">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartActivity" />
        <activity android:name=".user_sign.RegisterActivity"
            android:label="Register"/>
        <activity
            android:name=".user_sign.LoginActivity"
            android:label="Login" />
        <activity android:name=".CheckFIlesActivity"
            android:label="Check Files"/>
        <activity
            android:name=".CreateNotesActivity"
            android:label="Create File"></activity>
    </application>

</manifest>